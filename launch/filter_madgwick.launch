<?xml version="1.0"?>

<launch>
  <node name="madgwick_filter_node" pkg="imu_filter_madgwick" type="imu_filter_node">
    <remap from="imu/data" to="imu/madgwick_data"/>
    <remap from="imu/data_raw" to="imu/data"/>

    <param name="use_mag" type="bool" value="true"/>
    <param name="publish_tf" type="bool" value="false"/>
    <param name="fixed_frame" type="string" value="base_link"/>
    <param name="reverse_tf " type="bool" value="true"/>

  </node>
</launch>
